<!DOCTYPE html>
<html lang="es">
    <%- include('../partials/head') %>

<body>
    <%- include('../partials/header') %>

    <h1>Lista de Animales</h1>
    <ul>
        <% animals.forEach(animal => { %>
            <li>
                <a href="/adoptions/adoptDetail/<%= animal.id %>">
                    <%= animal.name %> - <%= animal.type %>
                </a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
